generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NFT {
  id        Int      @id @default(autoincrement())
  name      String
  imageUrl  String
  price     Float
  createdAt DateTime @default(now())
}

model Agent {
  id         Int      @id @default(autoincrement())
  name       String
  profileUrl String
  link       String
}
